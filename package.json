{
  "name": "unskool",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "clean": "rm -rf $TMPDIR/react-* && watchman watch-del-all && npm cache clean --force",
    "clean:android": "cd android/ && ./gradlew clean && cd .. && react-native run-android",
    "newclear": "rm -rf $TMPDIR/react-* && watchman watch-del-all && rm -rf ios/build && rm -rf node_modules/ && npm cache clean --force && npm i",
    "test:watch": "jest --watch",
    "updateSnapshot": "jest --updateSnapshot",
    "coverage": "jest --coverage && open coverage/lcov-report/index.html || xdg-open coverage/lcov-report/index.html",
    "android:build": "cd android && ./gradlew assembleRelease",
    "android:install": "cd android && ./gradlew assembleRelease && ./gradlew installRelease",
    "android:hockeyapp": "cd android && ./gradlew assembleRelease && puck -submit=auto app/build/outputs/apk/app-release.apk",
    "android:devices": "$ANDROID_HOME/platform-tools/adb devices",
    "android:logcat": "$ANDROID_HOME/platform-tools/adb logcat *:S ReactNative:V ReactNativeJS:V",
    "android:shake": "$ANDROID_HOME/platform-tools/adb devices | grep '\\t' | awk '{print $1}' | sed 's/\\s//g' | xargs -I {} $ANDROID_HOME/platform-tools/adb -s {} shell input keyevent 82",
    "storybook": "storybook start -p 7007",
    "lint": "standard --verbose | snazzy",
    "lintdiff": "git diff --name-only --cached --relative | grep '\\.js$' | xargs standard | snazzy",
    "fixcode": "standard --fix",
    "git-hook": "npm run lint -s && npm run test -s",
    "build:ios": "react-native bundle --entry-file='index.js' --bundle-output='./ios/main.jsbundle' --dev=false --platform='ios'",
    "create-android-appcenter": "appcenter apps create -d unskool-android -o Android -p React-Native",
    "create-ios-appcenter": "appcenter apps create -d unskool-ios -o iOS -p React-Native",
    "get-ios-details": "code-push deployment ls unskool-ios -k",
    "get-android-details": "code-push deployment ls unskool-Android -k",
    "release-android-appcenter": "appcenter codepush release-react -a unskool12-outlook.com/unskool-android -d Staging",
    "release-ios-appcenter": "appcenter codepush release-react -a unskool12-outlook.com/unskool-ios -d Production",
    "postinstall": "node ./android-release-gradle-fix.js",
    "r-a": "cd android && ./gradlew clean && ./gradlew ap:assembleRelease",
    "android-build-release": "cd ./android && rm -rf app/src/main/res/drawable* && ./gradlew app:assembleRelease"
  },
  "dependencies": {
    "@react-native-community/masked-view": "^0.1.10",
    "apisauce": "^1.1.1",
    "firebase": "^7.14.3",
    "format-json": "^1.0.3",
    "identity-obj-proxy": "^3.0.0",
    "lodash": "^4.17.15",
    "moment": "^2.25.3",
    "patch-package": "^6.2.2",
    "prop-types": "^15.7.2",
    "querystringify": "^2.1.1",
    "ramda": "^0.27.0",
    "ramdasauce": "^2.1.3",
    "react": "16.11.0",
    "react-native": "0.62.2",
    "react-native-action-button": "^2.8.5",
    "react-native-animatable": "^1.3.3",
    "react-native-best-viewpager": "^1.0.4",
    "react-native-button-component": "^0.2.28",
    "react-native-calendars": "1.300.0",
    "react-native-code-push": "^6.2.0",
    "react-native-config": "^1.0.0",
    "react-native-datepicker": "^1.7.2",
    "react-native-device-info": "^5.5.7",
    "react-native-dropdownalert": "^4.2.1",
    "react-native-easy-toast": "^1.2.0",
    "react-native-elements": "^2.0.0",
    "react-native-fast-image": "^8.1.5",
    "react-native-fs": "^2.16.6",
    "react-native-gesture-handler": "^1.6.1",
    "react-native-gifted-chat": "^0.16.0",
    "react-native-google-places-autocomplete": "^1.4.2",
    "react-native-hero": "^0.3.1",
    "react-native-image-picker": "^2.3.1",
    "react-native-image-resizer": "^1.2.2",
    "react-native-iphone-x-helper": "^1.2.1",
    "react-native-keyboard-aware-scroll-view": "^0.9.1",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-modal": "^11.5.6",
    "react-native-onesignal": "3.8.1",
    "react-native-orientation-loading-overlay": "^0.1.6",
    "react-native-parallax-scroll-view": "^0.21.3",
    "react-native-radio-buttons": "^1.0.0",
    "react-native-reanimated": "^1.9.0",
    "react-native-safe-area-context": "^1.0.0",
    "react-native-screens": "^2.7.0",
    "react-native-simple-radio-button": "^2.7.4",
    "react-native-snap-carousel": "^3.9.0",
    "react-native-stack": "^1.0.0-alpha11",
    "react-native-star-rating": "^1.1.0",
    "react-native-swiper": "^1.6.0",
    "react-native-tab-view": "^2.14.2",
    "react-native-vector-icons": "^6.6.0",
    "react-navigation": "3.13.0",
    "react-navigation-fluid-transitions": "^0.3.2",
    "react-navigation-redux-helpers": "^4.0.1",
    "react-navigation-stack": "^2.4.0",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.1.3",
    "redux-saga-firebase": "^0.15.0",
    "reduxsauce": "^1.1.2",
    "rn-fetch-blob": "^0.12.0",
    "seamless-immutable": "^7.1.4"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/runtime": "^7.6.2",
    "@react-native-community/eslint-config": "^0.0.5",
    "@storybook/addon-storyshots": "^5.3.18",
    "@storybook/addons": "^5.3.18",
    "@storybook/channels": "^5.3.18",
    "@storybook/react-native": "^5.3.18",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^24.9.0",
    "babel-plugin-ignite-ignore-reactotron": "0.3.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react-native": "^4.0.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.5.1",
    "ignite-andross": "^4.2.0",
    "ignite-animatable": "^1.0.2",
    "ignite-dev-screens": "^2.4.5",
    "ignite-elements": "^0.3.0",
    "ignite-redux-persist": "^2.0.0",
    "ignite-standard": "^1.0.0",
    "jest": "^24.9.0",
    "metro-react-native-babel-preset": "^0.58.0",
    "mockery": "^2.1.0",
    "react-devtools-core": "^4.6.0",
    "react-dom": "^16.13.1",
    "react-test-renderer": "16.11.0",
    "reactotron-react-native": "2.1.0",
    "reactotron-redux": "2.1.0",
    "reactotron-redux-saga": "^3.0.0",
    "snazzy": "^8.0.0",
    "standard": "^14.3.4"
  },
  "jest": {
    "testMatch": [
      "<rootDir>/Tests/**/*.js",
      "**/?(*.)(spec|test).js?(x)"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/Tests/Setup.js"
    ],
    "moduleNameMapper": {
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "identity-obj-proxy"
    },
    "transform": {
      "^.+\\.(js)$": "<rootDir>/node_modules/react-native/jest/preprocessor.js"
    },
    "setupFiles": [
      "<rootDir>/Tests/Setup"
    ],
    "preset": "react-native"
  },
  "config": {},
  "standard": {
    "parser": "babel-eslint",
    "globals": [
      "describe",
      "test",
      "jest",
      "expect",
      "fetch",
      "navigator",
      "__DEV__",
      "XMLHttpRequest",
      "FormData",
      "React$Element"
    ]
  }
}
